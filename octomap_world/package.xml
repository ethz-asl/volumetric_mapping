<?xml version="1.0"?>
<package format="2">
  <name>octomap_world</name>
  <version>0.0.0</version>
  <description>
      A wrapper for Octomap, allowing insertions of pointclouds and disparity maps.
      Inherits from WorldBase in volumetric_map_base package.
  </description>

  <author email="helen.oleynikova@mavt.ethz.ch">Helen Oleynikova</author>
  <author email="markus.achtelik_devel@mavt.ethz.ch">Markus Achtelik</author>

  <maintainer email="helen.oleynikova@mavt.ethz.ch">Helen Oleynikova</maintainer>

  <license>BSD</license>

  <buildtool_depend>catkin</buildtool_depend>
  <buildtool_depend>catkin_simple</buildtool_depend>

  <depend>minkindr_conversions</depend>
  <depend>octomap</depend>
  <depend>octomap_msgs</depend>
  <depend>octomap_ros</depend>
  <depend>pcl_conversions</depend>
  <depend>pcl_ros</depend>
  <depend>volumetric_map_base</depend>
  <depend>volumetric_msgs</depend>
  <depend>opencv3_catkin</depend>
</package>
